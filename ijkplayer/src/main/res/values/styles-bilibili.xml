<?xml version="1.0" encoding="utf-8"?>
<resources>
    <eat-comment />

    <style name="MediaButton">
        <item name="android:background">@null</item>
        <item name="android:layout_width">71dip</item>
        <item name="android:layout_height">52dip</item>
    </style>

    <style name="MediaButton.Previous">
        <item name="android:src">@android:drawable/ic_media_previous</item>
        <item name="android:contentDescription">@string/lockscreen_transport_prev_description</item>
    </style>

    <style name="MediaButton.Next">
        <item name="android:src">@android:drawable/ic_media_next</item>
        <item name="android:contentDescription">@string/lockscreen_transport_next_description</item>
    </style>

    <style name="MediaButton.Play_Pause">
        <item name="android:src">@drawable/selector_bilibili_play_pause</item>
        <item name="android:contentDescription">@string/lockscreen_transport_play_description</item>
    </style>

    <style name="MediaButton.Ffwd">
        <item name="android:src">@android:drawable/ic_media_ff</item>
        <item name="android:contentDescription">@string/lockscreen_transport_ffw_description</item>
    </style>

    <style name="MediaButton.Rew">
        <item name="android:src">@android:drawable/ic_media_rew</item>
        <item name="android:contentDescription">@string/lockscreen_transport_rew_description</item>
    </style>

    <style name="MediaButton.Pause">
        <item name="android:src">@drawable/ic_media_pause</item>
        <item name="android:contentDescription">@string/lockscreen_transport_pause_description</item>
    </style>


</resources>